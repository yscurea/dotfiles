{
	"workbench.colorTheme": "One Dark Pro Night Flat",
	"workbench.iconTheme": "material-icon-theme",
	"material-icon-theme.folders.theme": "specific",
	"editor.formatOnSave": true,
	"editor.inlayHints.enabled": "offUnlessPressed",
	"material-icon-theme.folders.associations": {
		"navigation": "cluster",
		"main": "App",
		"application": "App",
		"use_case": "App",
		"use_case_impl": "App",
		"infrastructure": "base",
		"domain": "Tasks",
		"entity": "Class",
		"acl": "connection",
		"exception": "error",
		"storage": "database",
		"setting": "Config",
		"repository": "Pipe",
		"repository_impl": "Pipe",
		"_generated": "include",
		"__mock__": "mock"
	},
	"extensions.experimental.affinity": {
		"asvetliakov.vscode-neovim": 1
	},
	"cSpell.userWords": [
		"aggr",
		"asvetliakov",
		"chrono",
		"drawio",
		"esbenp",
		"Fira",
		"fullscreen",
		"futureor",
		"hediet",
		"highlightedyank",
		"HOMEPATH",
		"marp",
		"mypy",
		"neovim",
		"nvim",
		"Pageless",
		"pylint",
		"rgba",
		"rmcp",
		"RobotoMono",
		"schemars",
		"serde",
		"todohighlight",
		"vimrc"
	],
	"vscode-neovim.compositeKeys": {
		"jj": {
			"command": "vscode-neovim.escape",
			"args": []
		},
		"jk": {
			"command": "vscode-neovim.escape",
			"args": []
		},
		"kk": {
			"command": "vscode-neovim.escape",
			"args": []
		},
		"kj": {
			"command": "vscode-neovim.escape",
			"args": []
		}
	},
	"vscode-neovim.neovimInitVimPaths.win32": "~/.config/nvim/init.lua",
	"todohighlight.keywords": [
		{
			"text": " XXX:",
			"color": "#f00",
			"backgroundColor": "#000a"
		},
		{
			"text": " FIXME:",
			"color": "#fff",
			"backgroundColor": "#f00c",
			"border": "2px solid #ff5555",
			"borderRadius": "0px"
		},
		{
			"text": " WARNING:",
			"color": "#fff",
			"backgroundColor": "#f000",
			"border": "2px solid #ff5555",
			"borderRadius": "5px"
		},
		{
			"text": " REVIEW:",
			"color": "#fff",
			"backgroundColor": "#fff0",
			"border": "2px solid #dd0",
			"borderRadius": "5px"
		},
		{
			"text": " NOTE:",
			"color": "#fff",
			"backgroundColor": "#fff0",
			"border": "1px solid #22ff77",
			"borderRadius": "5px"
		},
		{
			"text": " TODO:",
			"color": "#fff",
			"border": "3px solid #225aff",
			"borderRadius": "5px",
			"backgroundColor": "#fff0"
		},
		{
			"text": " HACK:",
			"color": "white",
			"border": "1px solid white",
			"borderRadius": "5px",
			"backgroundColor": "#222222"
		},
		{
			"text": " QUESTION:",
			"color": "#aaaa33",
			"border": "1px solid #888888",
			"borderRadius": "5px",
			"backgroundColor": "#222222"
		}
	],
	"todohighlight.isEnable": true,
	"todo-tree.general.tags": [
		"BUG",
		"HACK",
		"FIXME",
		"TODO",
		"XXX",
		"[ ]",
		"[x]",
		"NOTE",
		"WARNING",
		"REVIEW",
		"QUESTION"
	],
	"todo-tree.highlights.enabled": false,
	"[markdown]": {
		"editor.unicodeHighlight.ambiguousCharacters": false,
		"editor.unicodeHighlight.invisibleCharacters": false,
		"diffEditor.ignoreTrimWhitespace": false,
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.wordWrap": "on",
		"editor.quickSuggestions": {
			"comments": "off",
			"strings": "off",
			"other": "off"
		},
		"cSpell.fixSpellingWithRenameProvider": true,
		"cSpell.advanced.feature.useReferenceProviderWithRename": true,
		"cSpell.advanced.feature.useReferenceProviderRemove": "/^#+\\s/",
		"editor.formatOnSave": true
	},
	"markdown.preview.breaks": true,
	"markdown.marp.themes": ["./marp-themes/drives.css"],
	"files.exclude": {
		"**/.git": false
	},
	"workbench.editorAssociations": {
		"*.csv": "default"
	},
	"[python]": {
		"editor.codeActionsOnSave": {
			"source.organizeImports": "explicit"
		},
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "ms-python.black-formatter"
	},
	"ruff.enable": true,
	"python.analysis.typeCheckingMode": "strict",
	"[dart]": {
		"editor.formatOnSave": true,
		"editor.formatOnType": true,
		"editor.rulers": [80],
		"editor.selectionHighlight": false,
		"editor.suggest.snippetsPreventQuickSuggestions": false,
		"editor.suggestSelection": "first",
		"editor.tabCompletion": "onlySnippets",
		"editor.wordBasedSuggestions": "off",
		"editor.defaultFormatter": "Dart-Code.dart-code",
		"editor.formatOnPaste": true
	},
	"dart.flutterSelectDeviceWhenConnected": true,
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"terminal.integrated.defaultProfile.windows": "PowerShell",
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"**/*.code-search": true
	},
	"workbench.panel.defaultLocation": "right",
	"terminal.integrated.defaultLocation": "editor",
	"editor.fontSize": 18,
	"terminal.integrated.fontFamily": "RobotoMono Nerd Font Mono",
	"aws.telemetry": false,
	"yaml.customTags": [
		"!And",
		"!And sequence",
		"!If",
		"!If sequence",
		"!Not",
		"!Not sequence",
		"!Equals",
		"!Equals sequence",
		"!Or",
		"!Or sequence",
		"!FindInMap",
		"!FindInMap sequence",
		"!Base64",
		"!Join",
		"!Join sequence",
		"!Cidr",
		"!Ref",
		"!Sub",
		"!Sub sequence",
		"!GetAtt",
		"!GetAZs",
		"!ImportValue",
		"!ImportValue sequence",
		"!Select",
		"!Select sequence",
		"!Split",
		"!Split sequence"
	],
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true
	},
	"files.eol": "\n",
	"files.autoSave": "off",
	"window.titleBarStyle": "custom",
	"accessibility.signalOptions.volume": 0,
	"github.copilot.nextEditSuggestions.enabled": true
}

